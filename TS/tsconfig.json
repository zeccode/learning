{
  "compilerOptions": {
    "jsx": "react",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "moduleResolution": "node",
    "module": "ES6", // Ou "ES6" dependendo do que você está utilizando
    "experimentalDecorators": true,
    "outDir": "./dist",
    "target": "ES5",
  },
  "include": ["src/**/*"]
}
